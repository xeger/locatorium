{
  "name": "locatorium",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test:preview": "vitest --config vitest-preview.config.ts",
    "test:playwright": "vitest run --config vitest-playwright.config.ts",
    "test:playwright:ui": "vitest --config vitest-playwright.config.ts --browser.headless=false",
    "test:webdriverio": "vitest run --config vitest-webdriverio.config.ts",
    "test:webdriverio:ui": "vitest --config vitest-webdriverio.config.ts --browser.headless=false",
    "test-e2e": "playwright test",
    "test-e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "@radix-ui/react-context-menu": "^2.2.4",
    "@radix-ui/react-slot": "^1.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.469.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "3.0.0-beta.3",
    "@vitest/coverage-istanbul": "3.0.0-beta.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "playwright": "^1.49.1",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite": "^6.0.6",
    "vite-plugin-istanbul": "^6.0.2",
    "vitest": "3.0.0-beta.3",
    "vitest-browser-react": "^0.0.4",
    "webdriverio": "^9.4.5"
  }
}
